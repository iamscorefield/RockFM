{
  "name": "Rock City FM",
  "short_name": "RockCityFM",
  "id": "/RockFM/",
  "start_url": "/RockFM/index.html",
  "scope": "/RockFM/",
  "display": "standalone",
  "background_color": "#ffffff",
  "description": "Rock City FM Radio Web Application",
  "icons": [
    {
      "src": "/RockFM/icon-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "/RockFM/icon1.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "theme_color": "#000000",
  "prefer_related_applications": false,
  "dir": "ltr",
  "lang": "en",
  "categories": ["music", "radio", "entertainment"],
  "orientation": "any",
  "display_override": ["standalone", "window-controls-overlay"],
  "file_handlers": [
    {
      "action": "/RockFM/open-file",
      "accept": {
        "application/pdf": [".pdf"],
        "image/*": [".png", ".jpg", ".jpeg"]
      }
    }
  ],
  "handle_links": {
    "urls": ["https://iamscorefield.github.io/RockFM/*"]
  },
  "protocol_handlers": [
    {
      "protocol": "web+rockcityfm",
      "url": "/RockFM/?url=%s"
    }
  ],
  "share_target": {
    "action": "/RockFM/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "share-title",
      "text": "share-text",
      "url": "share-url",
      "files": [{
        "name": "share-file",
        "accept": ["*/*"]
      }]
    }
  },
  "shortcuts": [
    {
      "name": "Listen Live",
      "short_name": "Listen",
      "url": "/RockFM/listen",
      "icons": [{"src": "/RockFM/icon-192x192.png", "sizes": "192x192"}]
    },
    {
      "name": "Contact Us",
      "short_name": "Contact",
      "url": "/RockFM/contact",
      "icons": [{"src": "/RockFM/icon-192x192.png", "sizes": "192x192"}]
    }
  ],
  "widgets": [
    {
      "name": "Rock City FM Widget",
      "short_name": "RockCityWidget",
      "url": "/RockFM/widget",
      "icons": [{"src": "/RockFM/icon-192x192.png", "sizes": "192x192"}]
    }
  ],
  "launch_handler": {
    "client_mode": "auto"
  },
  "screenshots": [
    {
      "src": "/RockFM/screenshot1.png",
      "sizes": "640x480",
      "type": "image/png"
    },
    {
      "src": "/RockFM/screenshot2.png",
      "sizes": "640x480",
      "type": "image/png"
    }
  ],
  "iarc_rating_id": "IARC12345",
  "related_applications": [],
  "scope_extensions": [],
  "edge_side_panel": {
    "title": "Rock City FM",
    "description": "Listen to Rock City FM directly from the Edge side panel."
  }
}
